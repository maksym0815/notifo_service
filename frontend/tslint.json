{
  "extends": [
    "tslint-immutable", 
    "tslint-config-airbnb",
    "tslint-react-hooks"
  ],
  "rules": {
    "align": false,
    "import-name": false,
    "indent": [
      true,
      "spaces"
    ],
    "max-line-length": false,
    "no-else-after-return": false,
    "no-increment-decrement": false,
    "object-shorthand-properties-first": false,
    "ordered-imports": [
      true
    ],
    "quotemark": [
      true,
      "single"
    ],
    "ter-arrow-parens": false,
    "ter-indent": false,
    "variable-name": false
  },
  "linterOptions": {
    "exclude": [
      "src/app/service/*.*"
    ]
  }
}